generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" 
  url      = env("DATABASE_URL")
}


model Record {
  id          Int      @id @default(autoincrement())
  year        Int?
  lgaKey      Int?
  lgaDesc     String
  numerator   Int?
  denominator Int?
  indicator   Float?
  isNdp       Boolean  @default(false)

  indicatorCode String 
  
  @@map("records")
  @@unique([year, lgaKey, indicatorCode]) // 防止同指标重复插入
}
