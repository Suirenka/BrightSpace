generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" 
  url      = env("DATABASE_URL")
}


model Record {
  id          Int     @id @default(autoincrement())
  year        Int?
  lgaKey      Int?
  lgaDesc     String
  numerator   Int?
  denominator Int?
  indicator   Float?
   isNdp       Boolean @default(false)

  @@map("records") 
}
